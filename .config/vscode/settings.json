{
  "workbench.list.automaticKeyboardNavigation": false,
  "vim.autoindent": false,
  "vim.enableNeovim": true,
  "vim.foldfix": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.matchpairs": "(:),{:},[:],<:>",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.neovimUseConfigFile": false,
  "vim.showMarksInGutter": true,
  "vim.easymotion": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["Y"],
      "after": ["y", "$"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["s"],
      "after": ["leader", "leader", "s"]
    },
    {
      "before": ["<space>"],
      "commands": ["whichkey.show"]
    },
    {
      "before": ["Q"],
      "after": [":", "b", "d", "<CR>"]
    },
    {
      "before": ["g", "p"],
      "after": ["\"", "0", "p"]
    },
    {
      "before": ["g", "P"],
      "after": ["\"", "0", "P"]
    }
  ],
  "vim.easymotionJumpToAnywhereRegex": "[$&+:=?@#|<>^*()%!\\{\\}_-]",
  "workbench.iconTheme": "material-icon-theme",
  "editor.acceptSuggestionOnEnter": "off",
  "telemetry.telemetryLevel": "off",
  "explorer.compactFolders": false,
  "workbench.enableExperiments": true,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "update.mode": "none",
  "extensions.autoUpdate": false,
  "window.menuBarVisibility": "toggle",
  "keyboard.dispatch": "keyCode",
  "workbench.editor.untitled.hint": "hidden",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnSave": true,
  "javascript.suggest.names": false,
  "editor.lineNumbers": "relative",
  "terminal.integrated.commandsToSkipShell": ["whichkey.show"],
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.ignoreProcessNames": [],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<space>"],
      "commands": ["whichkey.show"]
    }
  ],
  "vim.overrideCopy": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.cursorSurroundingLines": 99,
  "hdinsight.disableOpenSurveyLink": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "workbench.activityBar.visible": false
}
